<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="android:Theme.NoTitleBar" />

    <style name="CustomDialog" parent="android:style/Theme.Dialog">
        <!-- <item name="android:windowFrame">@null</item> -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item> -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="WrapContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MatchParent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="FitWidth">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="FitHeight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="header.background" parent="FitWidth">
        <item name="android:layout_height">0dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_marginLeft">-1dp</item>
        <item name="android:layout_marginRight">-1dp</item>
        <item name="android:src">@drawable/titlebar</item>
    </style>

    <style name="headerIcon">
        <item name="android:layout_width">@dimen/header_icon_width</item>
        <item name="android:layout_height">@dimen/header_icon_height</item>
        <item name="android:layout_marginTop">@dimen/header_icon_margin_top</item>
        <item name="android:layout_marginRight">@dimen/header_icon_margin_right</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="dealTabs">
        <item name="android:layout_width">@dimen/deal_top_tab_width</item>
        <item name="android:layout_height">@dimen/deal_top_tab_height</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="information.Row" parent="FitWidth">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:weightSum">3</item>
    </style>

    <style name="information.Icon" parent="WrapContent">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="search_span">
        <item name="android:textColor">@color/blue_dodger</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="detailIcon">
        <item name="android:layout_width">@dimen/att_btn_width_heigth</item>
        <item name="android:layout_height">@dimen/att_btn_width_heigth</item>
        <item name="android:layout_margin">4dp</item>
    </style>

    <style name="mapButton">
        <item name="android:layout_width">@dimen/map_button_width</item>
        <item name="android:layout_height">@dimen/map_button_height</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_marginBottom">@dimen/map_button_margin_bottom</item>
        <item name="android:adjustViewBounds">true</item>
    </style>

    <style name="map.menuContainer" parent="WrapContent">
        <item name="android:layout_marginBottom">@dimen/map_button_margin_bottom</item>
        <item name="android:adjustViewBounds">true</item>
    </style>

    <style name="mapMenu">
        <item name="android:layout_width">@dimen/map_menu_width</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="mapMenu.edge" parent="mapMenu">
        <item name="android:layout_height">@dimen/map_menu_edge_height</item>
    </style>

    <style name="mapMenu.middle" parent="mapMenu">
        <item name="android:layout_height">@dimen/map_menu_middle_height</item>
    </style>

    <style name="filterButton">
        <item name="android:layout_width">@dimen/filter_button_width</item>
        <item name="android:layout_height">@dimen/filter_button_height</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="style_main_title">
        <item name="android:textColor">@color/text_color_white</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textStyle">bold</item>
        <!-- <item name="android:singleLine">true</item> -->
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_main_text">
        <item name="android:textColor">@color/text_color_white</item>
        <item name="android:textSize">22sp</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_main_text_black">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">24sp</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_sub_text">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">17sp</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_sub_text2">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">18sp</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_description">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">13sp</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_event_promo_info">
        <item name="android:textColor">@color/text_box_color_dark_orange</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <!-- <item name="android:maxLines">2</item> -->
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_promo_info">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="style_cart_info">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:typeface">sans</item>
    </style>

    <!-- default style for page indicator -->
    <bool name="default_circle_indicator_centered">true</bool>

    <color name="default_circle_indicator_fill_color">#FFFFFFFF</color>
    <color name="default_circle_indicator_page_color">#00000000</color>

    <integer name="default_circle_indicator_orientation">0</integer>

    <dimen name="default_circle_indicator_radius">3dp</dimen>

    <bool name="default_circle_indicator_snap">false</bool>

    <color name="default_circle_indicator_stroke_color">#FFDDDDDD</color>

    <dimen name="default_circle_indicator_stroke_width">1dp</dimen>

    <style name="CirclePageIndicator">
        <item name="centered">@bool/default_circle_indicator_centered</item>
        <item name="fillColor">@color/default_circle_indicator_fill_color</item>
        <item name="pageColor">@color/default_circle_indicator_page_color</item>
        <item name="orientation">@integer/default_circle_indicator_orientation</item>
        <item name="radius">@dimen/default_circle_indicator_radius</item>
        <item name="snap">@bool/default_circle_indicator_snap</item>
        <item name="strokeColor">@color/default_circle_indicator_stroke_color</item>
        <item name="strokeWidth">@dimen/default_circle_indicator_stroke_width</item>
    </style>

    <style name="ShareDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item> -->
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="direction_autocomplete_label">
        <item name="android:textColor">@color/text_color_grey</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="direction_autocomplete_widget">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:typeface">sans</item>
        <item name="android:background">@null</item>
    </style>

    <style name="direction_title">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">sans</item>
        <item name="android:background">@null</item>
    </style>

    <style name="direction_description">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">sans</item>
        <item name="android:background">@null</item>
    </style>

    <style name="direction_time">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">sans</item>
        <item name="android:background">@null</item>
    </style>

    <style name="direction_circle">
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">sans</item>
        <item name="android:background">@null</item>
    </style>

</resources>